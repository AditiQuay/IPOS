package com.quayintech.tasklib.model;

import java.util.ArrayList;
import java.util.List;

/**
 * Created by deepak.kumar1 on 03-05-2018.
 */

public class WeekUtils {
    public static List<Weeks> getWeeksList() {
        List<Weeks> alertList = new ArrayList<>();
        alertList.add(new Weeks(0, "Sunday", false));
        alertList.add(new Weeks(1, "Monday", false));
        alertList.add(new Weeks(2, "Tuesday", false));
        alertList.add(new Weeks(3, "Wednesday", false));
        alertList.add(new Weeks(4, "Thursday", false));
        alertList.add(new Weeks(5, "Friday", false));
        alertList.add(new Weeks(6, "Saturday", false));
        return alertList;
    }

    public static Weeks getDefaultWeeks() {
        return getWeeksList().get(3);
    }

}
