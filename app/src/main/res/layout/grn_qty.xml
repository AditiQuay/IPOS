<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/lGrn"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/dim_4"
    android:layout_marginLeft="@dimen/dim_4"
    android:layout_marginRight="@dimen/dim_4"
    android:background="@drawable/rectangle_child_white"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="start|center_vertical"
                android:orientation="vertical"
                android:padding="8dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/titilliumwebregular"
                    android:text="GRN Number" />

                <EditText
                    android:id="@+id/grnNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dim_4"
                    android:background="@color/transparent"
                    android:enabled="false"
                    android:fontFamily="@font/titilliumwebregular"
                    android:textColor="@color/dashboard_textcolor"
                    android:textSize="@dimen/dim_14" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="4dp"
                android:background="@drawable/ipos_line_black" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="start|center_vertical"
                android:orientation="vertical"
                android:padding="8dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/titilliumwebregular"
                    android:text="Total Items" />

                <EditText
                    android:id="@+id/et_totalItems"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/transparent"
                    android:enabled="false"
                    android:fontFamily="@font/titilliumwebregular"
                    android:textColor="@color/dashboard_textcolor"
                    android:textSize="@dimen/dim_14" />

            </LinearLayout>

        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@drawable/ipos_line_black" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="start|center_vertical"
                android:orientation="vertical"
                android:padding="8dp"
                android:paddingLeft="@dimen/dim_8"
                android:paddingRight="8dp"
                android:paddingTop="@dimen/dim_8">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/textViewReceivedDate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/titilliumwebregular"
                        android:text="Received Date *" />

                    <ImageView
                        android:id="@+id/ivReceivedDateCalender"
                        android:layout_width="@dimen/dim_18"
                        android:layout_height="@dimen/dim_18"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/calendar" />

                </RelativeLayout>


                <EditText
                    android:id="@+id/et_received_date"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="@dimen/dim_4"
                    android:background="@color/transparent"
                    android:clickable="true"
                    android:focusable="false"
                    android:fontFamily="@font/titilliumwebregular"
                    android:textColor="@color/dashboard_textcolor"
                    android:textSize="@dimen/dim_14" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="4dp"
                android:background="@drawable/ipos_line_black" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="start|center_vertical"
                android:orientation="vertical"
                android:padding="8dp"
                android:visibility="visible">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/titilliumwebregular"
                    android:text="Value" />

                <EditText
                    android:id="@+id/et_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/transparent"
                    android:enabled="false"
                    android:fontFamily="@font/titilliumwebregular"
                    android:textColor="@color/dashboard_textcolor"
                    android:textSize="@dimen/dim_14" />

            </LinearLayout>


        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@drawable/ipos_line_black" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/ipos_line_black" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="start|center_vertical"
                android:orientation="vertical"
                android:padding="8dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/titilliumwebregular"
                    android:text="Open Qty" />

                <EditText
                    android:id="@+id/poQty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dim_4"
                    android:background="@color/transparent"
                    android:enabled="false"
                    android:fontFamily="@font/titilliumwebregular"
                    android:textColor="@color/dashboard_textcolor"
                    android:textSize="@dimen/dim_14" />

            </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@drawable/ipos_line_black" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="start|center_vertical"
                android:orientation="vertical"
                android:padding="8dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/titilliumwebregular"
                    android:text="GRN Qty" />

                <EditText
                    android:id="@+id/openQty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dim_4"
                    android:background="@color/transparent"
                    android:enabled="false"
                    android:fontFamily="@font/titilliumwebregular"
                    android:textColor="@color/dashboard_textcolor"
                    android:textSize="@dimen/dim_14" />

            </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@drawable/ipos_line_black" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="start|center_vertical"
                android:orientation="vertical"
                android:padding="8dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/titilliumwebregular"
                    android:text="Balance Quantity" />

                <EditText
                    android:id="@+id/balanceQty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dim_4"
                    android:background="@color/transparent"
                    android:enabled="false"
                    android:fontFamily="@font/titilliumwebregular"
                    android:textColor="@color/dashboard_textcolor"
                    android:textSize="@dimen/dim_14" />

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>