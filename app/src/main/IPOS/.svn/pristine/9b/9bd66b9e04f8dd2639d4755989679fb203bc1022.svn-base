                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/dim_16"
                        android:gravity="center"
                        android:src="@drawable/shipment" />

                    <TextView
                        android:id="@+id/textLogistics"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="@dimen/dim_16"
                        android:layout_toRightOf="@+id/imageLogistics"
                        android:fontFamily="@font/titilliumwebregular"
                        android:gravity="center"
                        android:text="Logistics"
                        android:textColor="@color/colorPrimary" />


                </RelativeLayout>

                <include layout="@layout/view_layout_without_dot" />

