<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/rlCashTitle"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dim_45"
            android:layout_marginTop="@dimen/dim_16"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/cashIcon"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dim_8"
                android:background="@drawable/ic_action_cash" />

            <quay.com.ipos.ui.CustomTextView
                android:id="@+id/tvCOD"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dim_8"
                android:layout_marginRight="@dimen/dim_40"
                android:fontFamily="@font/titilliumwebregular"
                android:layout_toRightOf="@+id/cashIcon"
                android:text="@string/cash"
                android:textAllCaps="true"
                android:textColor="@color/colorPrimary"
                android:textSize="@dimen/text_size_15sp" />

            <quay.com.ipos.ui.CustomTextView
                android:id="@+id/tvAmount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:fontFamily="@font/titilliumwebregular"
                android:layout_marginRight="@dimen/dim_8"
                android:text="AMOUNT"
                android:textAllCaps="true"
                android:textColor="@color/colorPrimary"
                android:textSize="@dimen/text_size_15sp"
                app:textAllCaps="false" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/dim_8"
            android:background="@color/backgroundgrey" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dim_8"
            android:padding="@dimen/dim_4">

            <quay.com.ipos.ui.CustomTextView
                android:id="@+id/tvOpeningBalance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:fontFamily="@font/titilliumwebregular"
                android:layout_marginLeft="@dimen/dim_4"
                android:text="Opening balance"
                android:textAllCaps="false"
                android:textColor="@color/grey"
                android:textSize="@dimen/text_size_14sp"
                app:textAllCaps="false" />

            <quay.com.ipos.ui.CustomTextView
                android:id="@+id/tvOpeningBalanceAmount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:fontFamily="@font/titilliumwebregular"
                android:layout_marginRight="@dimen/dim_8"
                android:text="9292.00"
                android:textAllCaps="true"
                android:textColor="@color/grey"
                android:textSize="@dimen/text_size_14sp"
                app:textAllCaps="false" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/dim_8"
            android:background="@color/backgroundgrey" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dim_8"
            android:padding="@dimen/dim_4">

            <quay.com.ipos.ui.CustomTextView
                android:id="@+id/tvCashSales"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:fontFamily="@font/titilliumwebregular"
                android:layout_marginLeft="@dimen/dim_4"
                android:text="Cash sales"
                android:textAllCaps="false"
                android:textColor="@color/grey"
                android:textSize="@dimen/text_size_14sp"
                app:textAllCaps="false" />

            <quay.com.ipos.ui.CustomTextView
                android:id="@+id/tvCashSalesAmount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/titilliumwebregular"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dim_8"
                android:text="9292.00"
                android:textAllCaps="false"
                android:textColor="@color/grey"
                android:textSize="@dimen/text_size_14sp"
                app:textAllCaps="false" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/dim_8"
            android:background="@color/backgroundgrey" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dim_8"
            android:padding="@dimen/dim_4">

            <quay.com.ipos.ui.CustomTextView
                android:id="@+id/tvTotal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/titilliumwebregular"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dim_4"
                android:text="Total"
                android:textAllCaps="false"
                android:textColor="@color/grey"
                android:textSize="@dimen/text_size_16sp"
                android:textStyle="bold"
                app:textAllCaps="false" />

            <quay.com.ipos.ui.CustomTextView
                android:id="@+id/tvTotalAmount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dim_8"
                android:text="19892.00"
                android:fontFamily="@font/titilliumwebregular"
                android:textAllCaps="true"
                android:textColor="@color/grey"
                android:textSize="@dimen/text_size_14sp"
                android:textStyle="bold"
                app:textAllCaps="false" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/dim_8"
            android:background="@color/backgroundgrey" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dim_8"
            android:padding="@dimen/dim_4">

            <CheckBox
                android:id="@+id/checkBoxBalance"
                android:fontFamily="@font/titilliumwebregular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <quay.com.ipos.ui.CustomTextView
                android:id="@+id/tvBalanceInHand"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:fontFamily="@font/titilliumwebregular"
                android:layout_marginLeft="@dimen/dim_4"
                android:layout_toRightOf="@+id/checkBoxBalance"
                android:text="Balance in hand"
                android:textAllCaps="false"
                android:textColor="@color/grey"
                android:textSize="@dimen/text_size_14sp"
                app:textAllCaps="false" />

            <quay.com.ipos.ui.CustomEditText
                android:id="@+id/tieBalanceInHand"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/titilliumwebregular"
                android:layout_alignParentRight="true"
                android:layout_gravity="center"
                android:background="@null"
                android:gravity="right"
                android:hint="@string/enter_cash_amount"
                android:inputType="number"
                android:padding="5dp"
                android:textColor="@color/dark_text_color"
                android:textColorHint="@color/grey"
                android:textSize="14dp" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/dim_8"
            android:background="@color/backgroundgrey" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dim_8"
            android:padding="@dimen/dim_4">

            <quay.com.ipos.ui.CustomTextView
                android:id="@+id/tvDifference"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/titilliumwebregular"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dim_4"
                android:text="Difference"
                android:textAllCaps="false"
                android:textColor="@color/grey"
                android:textSize="@dimen/text_size_16sp"
                android:textStyle="bold"
                app:textAllCaps="false" />

            <quay.com.ipos.ui.CustomTextView
                android:id="@+id/tvDifferenceAmount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dim_8"
                android:fontFamily="@font/titilliumwebregular"
                android:text="9292.00"
                android:textAllCaps="true"
                android:textColor="@color/grey"
                android:textSize="@dimen/text_size_14sp"
                android:textStyle="bold"
                app:textAllCaps="false" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/dim_8"
            android:background="@color/backgroundgrey" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dim_8"
            android:padding="@dimen/dim_4">

            <quay.com.ipos.ui.CustomTextView
                android:id="@+id/tvBankDeposit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dim_4"
                android:fontFamily="@font/titilliumwebregular"
                android:text="Bank Deposit"
                android:textAllCaps="false"
                android:textColor="@color/grey"
                android:textSize="@dimen/text_size_14sp"
                app:textAllCaps="false" />

            <quay.com.ipos.ui.CustomTextView
                android:id="@+id/tvBankDepositAmount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dim_8"
                android:fontFamily="@font/titilliumwebregular"
                android:text="9292.00"
                android:textAllCaps="true"
                android:textColor="@color/grey"
                android:textSize="@dimen/text_size_14sp"
                app:textAllCaps="false" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/dim_8"
            android:background="@color/backgroundgrey" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dim_8"
            android:padding="@dimen/dim_4">

            <CheckBox
                android:id="@+id/checkBoxAgree"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/titilliumwebregular"
                android:buttonTint="@color/colorPrimary"
                android:text="I agree"
                android:textColor="@color/colorPrimary" />

        </RelativeLayout>

    </LinearLayout>
</RelativeLayout>